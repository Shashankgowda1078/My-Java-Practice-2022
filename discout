import java.util.*;
public class Main
{
	public static void main(String[] args) {
		Scanner sc=new Scanner(System.in);
	    float a=sc.nextFloat();
	    float b=sc.nextFloat();
	    float c=sc.nextFloat();
	    float d=a+b;
	    float e=(d*c)/100;
	    float f=d-e;
	     System.out.println(d);
	     System.out.println(f);
	    
	    System.out.println(e);
		
		}
}
